    "# Vẽ hình vuông sử dụng vòng lặp while\n",
    "import turtle\n",
    "t=turtle.Turtle()\n",
    "t.color(\"red\")\n",
    "a=int(input(\"Nhập kích thước cạnh: \"))\n",
    "edge=0\n",
    "while edge < 4:\n",
    "    t.forward(a)\n",
    "    t.left(90)\n",
    "    edge+=1\n",
    "    \n"
